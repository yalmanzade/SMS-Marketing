@{
    ViewData["Title"] = "Home Page";
}
<head>
    <title>My App</title>
</head>
<body>
    <h1>Welcome to My App</h1>
    <a href="/my/oauth" class="btn btn-primary">Continue with TikTok</a>

    <h2>Upload Video</h2>
    <form action="/my/upload" method="post" enctype="multipart/form-data">
        <div class="mb-3">
            <label for="videoFile" class="form-label">Video File</label>
            <input type="file" class="form-control" id="videoFile" name="videoFile" required>
        </div>
        <button type="submit" class="btn btn-primary">Upload</button>
    </form>

    <!-- Bootstrap CSS -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-Pxubxv9XJihF0+igEjMrtJPt/EnH68GV0/w0d/5BpJ4NfZbc0N4NQD0JtSPN2Qwv" crossorigin="anonymous">
    <!-- Bootstrap JS -->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.min.js" integrity="sha384-5luVf1jZv5HQ9WRp94czuPdVq3ySUDb8jMnLvlG1jkAAJpZ4vH8MQ4fBQFb7o4/X" crossorigin="anonymous"></script>
</body>